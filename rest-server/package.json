  "description": "Provides a REST interface for managing a single database instance",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=development nodemon --watch index.js --watch src --watch ../node-client -e js index",
    "start:test": "NODE_ENV=test nodemon --watch index.js --watch src --watch ../node-client -e js index",
    "test": "./node_modules/mocha/bin/mocha --watch",
