  "license": "MIT",
  "devDependencies": {
    "babel-eslint": "^8.2.3",
    "chai": "^4.1.2",
    "eslint": "^4.19.1",
    "eslint-config-react-app": "^2.1.0",
    "eslint-plugin-flowtype": "^2.49.3",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.9.1",
    "moment": "^2.22.2",
    "nodemon": "^1.17.5",
    "pg": "^7.4.3",
    "superagent": "^3.8.3"
  },
  "dependencies": {
    "@marcopeg/utils": "^1.8.9",
    "body-parser": "^1.18.3",
